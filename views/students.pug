extends layout
block content
    #event.container-fluid
      .wrap-title-event.row
        .col-md-6.col-lg-3.nit.nit1
          .div
            ul
              li
                a(href='#') ⏺ Dashboard
              li
                a(href='/register') ➕ Register a new student
              li
                a(href='#') 🎉 Events
              li
                a(href='/class') 🏛 Classes
              li
                a(href='/students') Students 🧒 
        .col-8.wow.fadeInUp
            .data-disp
                h2= title
                //- .datas
                //-     each data in datas
                //-         ol
                //-             li= data.name
                .table-responsive
                    table.table
                        thead
                            tr
                                th Name
                                th Class
                                th Age
                        tbody
                            each data in datas
                                tr
                                    td= data.name
                                    td= data.class
                                    td= data.Age